drop table person;
create table person(
num number primary key,
id varchar2(10),
name varchar2(20),
age number
);

drop sequence person_seq;
create sequence person_seq
start with 1
minvalue 1
increment by 1;

commit;

col id for a10
col name for a10
select * from person;

🔽 <properties> 아래에 추가
<repositories>
        <repository>
            <id>oracle</id>
            <url>https://repo.maven.apache.org/maven2</url>
        </repository>
    </repositories>

 🔽 <dependency> 아래에 추가
<dependency>
    <groupId>org.mybatis.spring.boot</groupId>
    <artifactId>mybatis-spring-boot-starter</artifactId>
    <version>3.0.5</version>
</dependency>