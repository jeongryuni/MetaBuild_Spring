drop table person;
create table person(
num number primary key,
id varchar2(10),
name varchar2(20),
age number
);

drop sequence person_seq;
create sequence person_seq
start with 1
minvalue 1
increment by 1;

commit;

col id for a10
col name for a10
select * from person;

